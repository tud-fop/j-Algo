#include <stdio.h>

int main()
{
  int x1, x2, x3;
  scanf("%i", &x1);
  scanf("%i", &x2);
  scanf("%i", &x3);

  x1=1;
  x2=0;
  
  while(x1>=x2){
    while(x1>x2){
      if(x3==2) x2=x2+x1;
      x2=x2+x1;
      if(x2 == x1){
        x3 = x2;
        x3 = x3 * 42;
        x2 = x3 / 21;
        x3 = x2;
        x2 = x1;
      }
      else
      {
        x3 = x2 + 42;
        x3 = x3 - x2;
      }
    }
    x1=x1-1;
  }

  printf("%d", x3);
  return 0;
}

